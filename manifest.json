{
  "name": "USAA Transaction Reader",
  "version": "1.0",
  "description": "This extension shows a Google Image search result for the current page",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["pageReader.js"]
    }
  ],
  "browser_action": {
    "default_title": "Read Transactions",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
